{
    "name": "cleaniquecoders/laravel-uuid",
    "description": "Use UUID for your Laravel application.",
    "keywords": ["package", "laravel", "uuid"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nasrul Hazim",
            "email": "nasrulhazim.m@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CleaniqueCoders\\LaravelUuid\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CleaniqueCoders\\LaravelUuid\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.3",
        "doctrine/dbal": "^2.10",
        "illuminate/auth": "^5.5|^5.6|^5.7|^5.8|^6.0|^7.0",
        "illuminate/support": "^5.5|^5.6|^5.7|^5.8|^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.5.*|3.6.*|3.7.*|3.8.*|4.*|5.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CleaniqueCoders\\LaravelUuid\\LaravelUuidServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
